{
	"$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.26/schema.json",
	"app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.yaml",
	"border": false,
	"border_colours": {
		"monocle": "#ff3399",
		"single": "#42a5f5",
		"stack": "#00a542"
	},
	"border_offset": -1,
	"border_width": 0,
	"cross_monitor_move_behaviour": "Insert",
	"default_container_padding": 0,
	"default_workspace_padding": 0,
	"monitors": [
		{
			"workspaces": [
				{
					"layout": "BSP",
					"name": "I"
				},
				{
					"layout": "VerticalStack",
					"name": "II"
				},
				{
					"layout": "HorizontalStack",
					"name": "III"
				},
				{
					"layout": "UltrawideVerticalStack",
					"name": "IV"
				},
				{
					"layout": "Rows",
					"name": "V"
				},
				{
					"layout": "Grid",
					"name": "VI"
				},
				{
					"layout": "RightMainVerticalStack",
					"name": "VII"
				}
			]
		}
	],
	"stackbar": {
		"height": 40,
		"mode": "Never",
		"tabs": {
			"background": "#141414",
			"focused_text": "#00a542",
			"unfocused_text": "#b3b3b3",
			"width": 300
		}
	},
	"window_hiding_behaviour": "Cloak"
}
